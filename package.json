    "deploy": "node scripts/deploy.js upload",
    "watch": "node scripts/deploy.js watch",
    "build": "node build/build.js;rm -rf ../assets/app;cp -r dist ../assets;mv ../assets/dist ../assets/app",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
